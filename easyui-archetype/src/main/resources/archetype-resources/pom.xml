			<artifactId>struts2-json-plugin</artifactId> <version>2.3.16.3</version> 
			</dependency> -->

		<!-- Hibernate核心包 -->
		<dependency>
			<groupId>org.hibernate</groupId>
			<artifactId>hibernate-core</artifactId>
			<version>${hibernate.version}</version>
		</dependency>
